<Window x:Class="ProjectGFN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectGFN"
        mc:Ignorable="d"
        Title="Git4Nextop" Height="450" Width="800">
    <Grid>
        <Button x:Name="xClone" Content="Clone" HorizontalAlignment="Left" Margin="20,332,0,0" VerticalAlignment="Top" Width="121" Height="63" Click="xClone_Click"/>
        <TabControl HorizontalAlignment="Left" Height="240" Margin="197,25,0,0" VerticalAlignment="Top" Width="498">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Button x:Name="xRepo" Content="Select repository" HorizontalAlignment="Left" Margin="20,25,0,0" VerticalAlignment="Top" Width="121" Height="63" Click="xRepo_Click"/>

    </Grid>
</Window>
