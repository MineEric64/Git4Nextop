<Window x:Class="ProjectGFN.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectGFN"
        mc:Ignorable="d"
        Title="LoginWindow" Height="135.6" Width="188">
    <Grid>
        <Button x:Name="xLogin" Content="Login" HorizontalAlignment="Left" Margin="31,56,0,0" VerticalAlignment="Top" Width="120" Click="xLogin_Click"/>
        <PasswordBox x:Name="xToken" HorizontalAlignment="Left" Margin="31,33,0,0" VerticalAlignment="Top" Width="120" PasswordChar="*"/>

    </Grid>
</Window>
