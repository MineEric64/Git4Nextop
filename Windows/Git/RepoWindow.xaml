<Window x:Class="ProjectGFN.Windows.Git.RepoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectGFN.Windows.Git"
        mc:Ignorable="d"
        Title="Select Repository" Height="450" Width="288.7">
    <Grid>
        <TreeView x:Name="xRepos" HorizontalAlignment="Left" Height="315" Margin="20,25,0,0" VerticalAlignment="Top" Width="240"/>
        <Button x:Name="xOk"  Content="OK" HorizontalAlignment="Left" Margin="20,360,0,0" VerticalAlignment="Top" Width="240" Height="37" Click="xOk_Click"/>

    </Grid>
</Window>
